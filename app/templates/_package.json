{
  "name": "<%= projectName %>",
  "description": "<%= description %>",
  "version": "0.1.0",
  "author": "<%= authorName %>",
  "scripts": {
    "postinstall" : "typings install && npm run tsc",
    "start": "npm run tslint && typings install && concurrently \"npm run tsc:w\" \"npm run serve\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "serve": "node browser-sync-config.js",
    "typings": "typings",
    "tslint": "npm run tslint-src && npm run tslint-root",
    "tslint-src": "tslint -c tslint.json app/**/*.ts",
    "tslint-root": "tslint -c tslint.json *.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubAccount %>/<%= projectName %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= githubAccount %>/<%= projectName %>/issues"
  },
  "dependencies": {
    "alfresco-js-api": "^0.1.0",
    "angular2": "2.0.0-beta.15",
    "es6-shim": "^0.35.0",
    "flag-icon-css": "^2.3.0",
    "material-design-icons": "^2.2.3",
    "material-design-lite": "^1.1.3",
    "ng2-alfresco-core": "^0.1.0",
    "ng2-alfresco-datatable": "^0.1.0",
    "ng2-alfresco-documentlist": "^0.1.0",
    "ng2-alfresco-login": "^0.1.0",
    "ng2-alfresco-search": "^0.1.9",
    "ng2-alfresco-upload": "^0.1.0",
    "ng2-alfresco-viewer": "^0.1.12",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.26",
    "zone.js": "0.6.10",
    "ng2-translate": "1.11.1",
    "ng2-charts": "1.0.3",
    "chart.js": "^2.1.4"
  },
  "devDependencies": {
    "browser-sync": "^2.10.0",
    "connect-history-api-fallback": "^1.2.0",
    "concurrently": "^2.0.0",
    "del": "^2.1.0",
    "tslint": "^3.8.1",
    "typescript": "^1.8.10",
    "typings": "^0.7.12",
    "license-check": "^1.0.4"
  },
  "publishConfig": {
    "registry": "http://devproducts.alfresco.me:4873/"
  }
}
